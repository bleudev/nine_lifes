{
  "schemaVersion": 1,
  "id": "nine_lifes",
  "version": "${version}",
  "name": "Nine lifes",
  "description": "A mod for Minecraft which gives you in survival only 9 lifes before death forever.",
  "authors": ["bleudev"],
  "contact": {
    "homepage": "https://github.com/bleudev/nine_lifes",
    "sources": "https://github.com/bleudev/nine_lifes.git",
    "issues": "https://github.com/bleudev/nine_lifes/issues"
  },
  "license": "MIT",
  "icon": "assets/nine_lifes/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.bleudev.nine_lifes.client.NineLifesClient"
    ],
    "modmenu": [
      "com.bleudev.nine_lifes.client.compat.modmenu.NineLifesModMenuApi"
    ],
    "main": [
      "com.bleudev.nine_lifes.NineLifes"
    ]
  },
  "mixins": [
    "nine_lifes.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version} <${max_exc_version}",
    "midnightlib": ">=${min_midnightlib_version}"
  }
}
